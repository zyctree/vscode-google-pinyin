{
    "name": "google-pinyin",
    "publisher": "zyctree",
    "repository": {
        "type": "git",
        "url": "https://github.com/zyctree/vscode-google-pinyin"
    },
    "displayName": "google-pinyin",
    "description": "Input Chinese with online google input tools",
    "version": "0.1.5",
    "engines": {
        "vscode": "^1.36.0"
    },
    "categories": [
        "Keymaps",
        "Other"
    ],
    "activationEvents": [
        "onCommand:google-pinyin.toggle"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "google-pinyin.toggle",
                "title": "Toogle Google Pinyin"
            }
        ],
        "keybindings": [
            {
                "command": "google-pinyin.toggle",
                "key": "ctrl+space"
            },
            {
                "command": "google-pinyin.accept.seleted",
                "key": "enter",
                "when": "google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.input.unsolved",
                "key": "\\",
                "when": "google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.accept.first",
                "key": "space",
                "when": "google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.pagenext",
                "key": "=",
                "when": "google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.pageprev",
                "key": "-",
                "when": "google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.pagenext",
                "key": "pagedown",
                "when": "google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.pageprev",
                "key": "pageup",
                "when": "google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.1",
                "key": "1",
                "when": "google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.2",
                "key": "2",
                "when": "google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.3",
                "key": "3",
                "when": "google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.4",
                "key": "4",
                "when": "google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.5",
                "key": "5",
                "when": "google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.6",
                "key": "6",
                "when": "google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.7",
                "key": "7",
                "when": "google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.8",
                "key": "8",
                "when": "google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.a",
                "key": "a",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.b",
                "key": "b",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.c",
                "key": "c",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.d",
                "key": "d",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.e",
                "key": "e",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.f",
                "key": "f",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.g",
                "key": "g",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.h",
                "key": "h",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.i",
                "key": "i",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.j",
                "key": "j",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.k",
                "key": "k",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.l",
                "key": "l",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.m",
                "key": "m",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.n",
                "key": "n",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.o",
                "key": "o",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.p",
                "key": "p",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.q",
                "key": "q",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.r",
                "key": "r",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.s",
                "key": "s",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.t",
                "key": "t",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.u",
                "key": "u",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.v",
                "key": "v",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.w",
                "key": "w",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.x",
                "key": "x",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.y",
                "key": "y",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            },
            {
                "command": "google-pinyin.typing.z",
                "key": "z",
                "when": "editorTextFocus && google-pinyin.enabled && !google-pinyin.seleting"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.14.15",
        "@types/vscode": "~1.36.0",
        "glob": "^7.1.4",
        "mocha": "^10.2.0",
        "tslint": "^5.12.1",
        "typescript": "^3.5.3",
        "vscode-test": "^1.2.0"
    }
}
